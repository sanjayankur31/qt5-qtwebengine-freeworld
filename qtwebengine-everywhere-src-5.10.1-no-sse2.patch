+void TurboAssembler::X87SetFPUCW(int cw) {
+  RecordComment("-- X87SetFPUCW start --");
+  push(Immediate(cw));
+  fldcw(MemOperand(esp, 0));
+  add(esp, Immediate(kPointerSize));
+  RecordComment("-- X87SetFPUCW end--");
+}
