+void TurboAssembler::TruncateX87TOSToI(Zone* zone, Register result_reg) {
+  sub(esp, Immediate(kDoubleSize));
+  fst_d(MemOperand(esp, 0));
+  SlowTruncateToIDelayed(zone, result_reg, esp, 0);
+  add(esp, Immediate(kDoubleSize));
+}
